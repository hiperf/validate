{
    "name": "@repharm/validate",
    "version": "1.0.0",
    "description": "json schema validator",
    "main": "index.js",
    "license": "MIT",
    "exports": {
        ".": {
            "require": "./dist/cjs/index.js",
            "import": "./dist/es/index.js"
        },
        "./slim": {
            "require": "./dist/cjs/slim.js",
            "import": "./dist/es/slim.js"
        },
        "./create": {
            "require": "./dist/cjs/create.js",
            "import": "./dist/es/create.js"
        },
        "./*": {
            "require": "./dist/cjs/validators/*",
            "import": "./dist/es/validators/*"
        }
    },
    "devDependencies": {
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-replace": "^5.0.7",
        "@rollup/plugin-terser": "^0.4.4",
        "jsdoc-to-markdown": "^8.0.1",
        "rollup-plugin-bundle-size": "^1.0.3",
        "vitepress": "^1.3.0",
        "vitest": "^2.0.1",
        "vue": "^3.4.31"
    },
    "scripts": {
        "test": "vitest --run",
        "test:watch": "vitest",
        "build": "rollup -c",
        "build:production": "rollup -c --config-minify && node ./utils/genDocsVaribles.js",
        "build:watch": "rollup -c --watch",
        "docs": "node ./utils/docs.js",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs"
    }
}
